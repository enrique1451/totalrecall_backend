{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/userAuth.json",
  "type": "object",
  "username": {
    "$id": "/properties/username",
    "type": "string",
    "title": "The Username Schema ",
    "default": ""
  },
  "password": {
    "$id": "/properties/password",
    "type": "string",
    "title": "The Password Schema ",
    "default": ""

  },
  
  "additionalProperties": false,
  "required": [
    "username",
    "password"
  ]
}
